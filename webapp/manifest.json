{
    "_version": "1.1.0",
    "sap.app": {
        "_version": "1.1.0",
        "id": "hcm.fab.mytimeevents.com.erco.timeevents",
        "type": "application",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "sourceTemplate": {
            "id": "@sap/ux-app-migrator:extension",
            "version": "1.13.2",
            "toolsId": "e81744b3-bed1-46bd-8994-b1918c9203b9"
        },
        "dataSources": {
			"timeEventService": {
				"uri": "/sap/opu/odata/sap/ZHCMFAB_MYTIMEEVENTS_EXT_SRV/",
				"settings": {
					"localUri": "./localService/metadata.xml"
				}
			}
		}
    },
    "sap.ui": {
        "_version": "1.1.0",
        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://time-account",
            "favIcon": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "sap_hcb",
            "sap_belize"
        ]
    },
    "sap.ui5": {
        "_version": "1.1.0",
        "dependencies": {
            "minUI5Version": "1.44.9"
        },
        "extends": {
            "component": "hcm.fab.mytimeevents",
            "extensions": {
                
				"sap.ui.controllerExtensions": {
					"hcm.fab.mytimeevents.controller.Overview": {
						"controllerName": "hcm.fab.mytimeevents.com.erco.timeevents.controller.OverviewCustom"
					}
				}
			}
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        }
    }
}