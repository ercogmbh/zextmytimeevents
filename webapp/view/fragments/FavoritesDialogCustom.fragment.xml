<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
 <SelectDialog id="favoriteDialog" title="{i18n>selectEventTypes}" noDataText="{i18n>nodata}" cancel="handleClose"
  confirm="handleConfirm" items="{timeEventTypeFav>/}" multiSelect="true" liveChange="handleSearch" growingThreshold="1000" >
  <StandardListItem title="{timeEventTypeFav>TimeTypeText}" description="{timeEventTypeFav>TimeType}" selected="{timeEventTypeFav>selected}" icon="sap-icon://fob-watch">
   <customData>
    <core:CustomData key="TimeType" value="{timeEventTypeFav>TimeType}"/>
    <core:CustomData key="TimeTypeText" value="{timeEventTypeFav>TimeTypeText}"/>
   </customData>
  </StandardListItem>
  <!--</List>-->
 </SelectDialog>
</core:FragmentDefinition>